{
    "name": "financehq-take-home",
    "version": "1.0.0",
    "main": "dist/index.js",
    "scripts": {
      "start": "node dist/index.js",
      "dev": "ts-node-dev --respawn src/index.ts",
      "build": "tsc",
      "test": "jest",
      "test:watch": "jest --watch"
    },
    "dependencies": {
      "apollo-server-express": "^3.12.1",
      "express": "^4.18.2",
      "graphql": "^16.8.1"
    },
    "devDependencies": {
      "@types/express": "^4.17.21",
      "@types/jest": "^29.5.5",
      "@types/node": "^20.10.0",
      "@types/supertest": "^2.0.12",
      "jest": "^29.7.0",
      "supertest": "^6.3.3",
      "ts-jest": "^29.1.1",
      "ts-node-dev": "^2.0.0",
      "typescript": "^5.3.2"
    },
    "jest": {
      "preset": "ts-jest",
      "testEnvironment": "node",
      "roots": [
        "<rootDir>/src"
      ],
      "testMatch": [
        "**/__tests__/**/*.test.ts"
      ]
    }
  }
  